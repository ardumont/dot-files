# -*- sh -*-

if [ $DISTRIB = "debian" ]; then

    [ -f /usr/share/zsh/vendor_completions/_tmuxinator ] && source /usr/share/zsh/vendor_completions/_tmuxinator
    [ -f /usr/share/autojump/autojump.zsh ] && source /usr/share/autojump/autojump.zsh

elif [ $DISTRIB = "nixos" ]; then

fi

# autojump
[ -f $AUTOJUMP_HOME/share/autojump/autojump.zsh ] && . $AUTOJUMP_HOME/share/autojump/autojump.zsh
