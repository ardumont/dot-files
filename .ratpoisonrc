# binding to call programs
bind x   exec gnome-terminal --hide-menubar -x tmux
bind e   exec ~/bin/emacs.sh
bind c   exec conkeror
bind ,   exec gnome-control-center
bind .   exec totem
bind /   exec transmission-gtk
bind i   exec ~/bin/ide-eclipse.sh
bind j   exec ~/bin/idea.sh
bind n   exec nautilus
bind y   exec ~/bin/yed.sh
bind C-f exec ~/applications/fritzing/Fritzing
bind C-v exec virtualbox

bind C-c exec arduino
bind C-o exec eog
bind C-s exec gnome-screenshot
bind C-e exec evince
bind C-f exec firefox
bind C-b exec google-chrome

# switch between screens
bind x nextscreen
binx z prevscreen

# start some programs by default
gnome-power-manager&
gnome-panel&
gnome-terminal --hide-menubar -x tmux&
