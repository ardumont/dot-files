Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintlight
Xft.rgba: rgb

! rxvt-unicode setup
URxvt*termName: screen-256color
URxvt.letterSpace: -1
URxvt*font: xft:DejaVu Sans Mono:pixelsize=12:antialias=true:hinting=true
URxvt*boldFont: xft:DejaVu Sans Mono:bold:antialias=true:hinting=true:pixelsize=12
URxvt*italicFont: xft:DejaVu Sans Mono:italic:hinting=true:pixelsize=12
URxvt*boldItalicFont: xft:DejaVu Sans Mono:bold:italic:hinting=true:pixelsize=12

URxvt*scrollBar: false
URxvt*scrollBar_right: false
URxvt*scrollBar_floating: false
URxvt*background: Black
URxvt*foreground: White
URxvt*colorUL: yellow
URxvt*underlineColor: yellow
